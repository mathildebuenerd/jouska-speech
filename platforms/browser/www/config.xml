<?xml version='1.0' encoding='utf-8'?>
<widget id="com.quantifiedtalk.test" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Jouska</name>
    <description>
        Messaging app that analyzes your emotions.
    </description>
    <author email="mathilde.buenerd@gmail.com" href="http://chilidesign.fr">
        Math
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
